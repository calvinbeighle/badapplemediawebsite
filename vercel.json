{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/lovable-uploads/(.*)",
      "dest": "/lovable-uploads/$1",
      "continue": true
    },
    {
      "src": "/(.*)\\.(.+)$",
      "dest": "/$1.$2",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 